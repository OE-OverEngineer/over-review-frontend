{
  "name": "over-review",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test:snapshots": "jest --updateSnapshot",
    "test:ci": "jest --all --ci --detectOpenHandles",
    "test:pr": "jest --coverage --changedSince=origin/main",
    "test:watch": "jest --watch",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write \"src/**/*.+(ts|js|tsx)\" --ignore-path .gitignore   --config ./.prettierrc.js",
    "postinstall": "husky install",
    "commit": "cz"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@tailwindcss/line-clamp": "^0.2.2",
    "antd": "^4.17.0-alpha.7",
    "axios": "^0.24.0",
    "buffer": "^6.0.3",
    "dayjs": "^1.10.7",
    "next": "11.1.0",
    "next-plugin-antd-less": "^1.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-slick": "^0.28.1",
    "react-toastify": "^8.1.0",
    "slick-carousel": "^1.8.1",
    "sweetalert2": "^11.1.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@types/enzyme": "^3.10.10",
    "@types/jest": "^27.0.3",
    "@types/react": "17.0.18",
    "@types/react-slick": "^0.23.7",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.5",
    "autoprefixer": "^10.3.1",
    "babel-jest": "^27.3.1",
    "babel-plugin-import": "^1.13.3",
    "css-loader": "^6.5.1",
    "cz-conventional-changelog": "^3.3.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "jest-svg-transformer": "^1.0.0",
    "jest-transform-stub": "^2.0.0",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lint-staged": "^11.1.2",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2",
    "react-test-renderer": "^17.0.2",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "tailwindcss": "^2.2.7",
    "ts-jest": "^27.0.7",
    "typescript": "4.3.5",
    "url-loader": "^4.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
